{%extends '::base-office.html.twig'%}
{%block active%}
    {{ render(controller("DefaultBundle:Default:menuAdmin",{'active':5})) }}
    {% block css %}
        <link src="{{asset('js/datetimepicker/build/css/bootstrap-datetimepicker.min.css')}}" rel="stylesheet" />
    {% endblock %}
{%endblock%}
{%block body%}
    <div class="container">
       
    </div>
    <div class="panel-body">
            <div class=" row col-lg-12">
                <div class = "col-md-12">
                    {% for type, messages in app.session.flashBag.all %}
                        {% for message in messages %}
                            <div class="alert alert-info">
                                {{ message|trans({}, 'FOSUserBundle') }}
                            </div>
                        {% endfor %}
                    {% endfor %}
                </div>
                
            </div>
     
        {% for message in app.session.flashbag.get('error') %}            
            <div class="col-lg-12 text-center m-t-md alert alert-warning fade in">
                <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                <strong>{{ message }}</strong>
            </div>
        {% endfor %}
        {% block fos_user_content %}
        {% endblock fos_user_content %}
    </div>
{%endblock%}
