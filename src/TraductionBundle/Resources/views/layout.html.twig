{%extends '::base.html.twig' %}
    {% block stylesheets %}
        {{parent()}}
            <link rel="stylesheet" href="{{asset('/css/bootstrap.min.css')}}">
            <link rel="stylesheet" href="{{ asset('bundles/lexiktranslation/ng-table/ng-table.min.css') }}">
        {% endblock %}
   {%block body%}  
       {% block flash_message %}
            <div class="container" style="width:100%">
                <div class="row">
                    <div class="col-md-10">
                        {% set flashes = app.session.flashbag.all() %}
                        {% if flashes | length > 0 %}
                            {% for type, messages in flashes %}
                                {% for message in messages %}
                                    <div class="alert alert-{{ type }}">{{ message }}</div>
                                {% endfor %}
                            {% endfor %}
                        {% endif %}
                    </div>
                </div>
            </div>
                    <div class="hpanel table-responsive">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-lg-12">
                        <h1 class="page-header">
                            {%trans%}internationalisation{%endtrans%}
                        </h1>
                        <ol class="breadcrumb">
                            <li>
                                <i class="fa fa-dashboard"></i>  <a href="{{path('administration')}}">{%trans%}dashboard{%endtrans%}</a>
                            </li>
                            <li>
                                <i class="fa fa-flag-o"></i><a href="{{path('lexik_translation_grid')}}">&nbsp;&nbsp{%trans%}traduction{%endtrans%}</a>
                            </li>
                            
                        </ol>
                    </div>
                </div>
            </div>
            <div class="panel-body" >
        {% endblock flash_message %}
       
        {% block content '' %}

        {% block javascript_footer %}
            <script src="{{asset('js/jquery.js')}}"></script>
            <script src="{{asset('js/angular.min.js')}}"></script>
            <script src="{{ asset('bundles/lexiktranslation/ng-table/ng-table.min.js') }}"></script>
        {% endblock %}
        </div>
  {%endblock%}