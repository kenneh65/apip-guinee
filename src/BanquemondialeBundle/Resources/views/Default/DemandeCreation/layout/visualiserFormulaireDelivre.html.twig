    <body>
    <style type="text/css">

		
		body {
			font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;
			font-size: 14px;
			line-height: 1.42857143;
			color: #333;
		}

		.page-header {
			padding-bottom: 9px;
			
			border-bottom: 1px solid #eee;
		}	
		
		.h1, h1 {
			font-size: 36px;	
			font-weight: 500;
			line-height: 1.1;
		}
		
		.form-group {
			margin-bottom: 15px;
		}
		.col-lg-12 {
			width: 100%;
		}
		
		.col-sm-6 {
			width: 50%;
			float: left;
			position: relative;
			min-height: 1px;
			padding-right: 15px;
			padding-left: 15px;
			
		}
		
		label {
			display: inline-block;
			max-width: 100%;
			margin-bottom: 5px;
			font-weight: bold;
		}
		
	.titreLabel
	{
		font-weight:bold;
		font-size:14px;
		line-height:25px;
	}
	
	.titreLabel1
	{
		font-weight:bold;
		font-size:14px;
		line-height:25px;
		text-align:right
	}
	
	.reponseLabel
	{
		font-weight:normal;
		font-size:14px;
		line-height:25px;
	}
	
	.cellValue
	{
		padding: 0 0 20px 0;
		font-size:14px;
	}
	
	.traitMince
	{
		color:silver;
	
	}
	
	.espacer
	{
		margin-left:50px;
	}
		
</style>
	
	
	

<div class="" style="width:100%;margin:0 auto 0 auto">
	<div  style="text-align: center;">
        <h1 class="page-header">
           {{ formulaireDelivre[0].idPole|trans}}  - {{ formulaireDelivre[0].idPole.nomFormulaire|trans }}
        </h1>
		<hr class="traitMince" noshade>
    </div>
						
    <br />
	<div class="col-md-12" style="margin:0 0 40px 0">
	<table style="width:100%">
	<tr>
		<th style="width:5%"></th>
		<th style="width:20%"></th>
		<th style="width:5%"></th>
		<th style="width:70%"></th>     
	</tr>
	<tr>
		<td></td>
		<td class="titreLabel1">{%trans%}message_dossier_numero{%endtrans%}:</td>
		<td></td>
		<td class="titreLabel espacer">{{ numeroDossier}}</td> 
	</tr>
	<tr><td style="height: 20px;"> </td></tr>
		{% for a in formulaireDelivre %}
		    <tr>
				<td></td>
				<td class="titreLabel">{{ a.libelle }}:</td>
				<td></td>
				<td class="titreLabel espacer">{{ a.valeur }}</td> 
			</tr>	
	<tr><td style="margin:5px 0 0 0"> </td></tr>			
		{% endfor %}
		<tr>
		<td></td>
		<td class="titreLabel">{%trans%}denomination{%endtrans%}:</td>
		<td></td>
		<td class="titreLabel espacer">{{ denominationSociale}}</td> 
	</tr>
	<tr><td></td></tr>
	<tr>
		<td></td>
		<td class="titreLabel">{%trans%}message_date_creation{%endtrans%}:</td>
		<td></td>
		<td class="titreLabel espacer">{{ date|date('d-m-Y')}}</td> 
	</tr>
	</table>
	</div>
</div>                
</body>
 
      