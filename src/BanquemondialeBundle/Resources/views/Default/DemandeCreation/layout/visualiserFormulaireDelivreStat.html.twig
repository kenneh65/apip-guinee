<body>
    <style type="text/css">		



		
		.h1, h1 {
			font-size: 36px;	
			font-weight: 500;
			line-height: 1.1;
		}
		
		.form-group {
			margin-bottom: 15px;
		}


		
		label {
			display: inline-block;
			max-width: 100%;
			margin-bottom: 5px;
			font-weight: bold;
		}
		
	.titreLabel
	{
		font-weight:bold;
		font-size:14px;
		line-height:25px;
	}
	
	.titreLabel1
	{
		font-weight:bold;
		font-size:14px;
		line-height:25px;
		text-align:right
	}
	
	.reponseLabel
	{
		font-weight:normal;
		font-size:14px;
		line-height:25px;
	}
	
	.cellValue
	{
		padding: 0 0 20px 0;
		font-size:14px;
	}
	
	.traitMince
	{
		color:silver;
	
	}
	
	.espacer
	{
		margin-left:50px;
	}
	
	#blocTitre
	{
		border: 5px double black;
		background-color: #E3E6E5;
		margin: 50px 0px 0px 0px;
		text-transform: uppercase;
		text-align:center;
	}
	
	#numDossier
	{
		margin: 36px 15px 0px 70%;
		float: right;
		white-space:nowrap;
	}
	
	.titreInput
	{
		font-size: 14px;
		width: 200px;
		 
	}
	
	.valeurInput
	{
	
	}
	
	.labelTitreBloc
	{
		text-transform: uppercase;
		font-size: 14px;
		padding: 14px 0 0 20px;
	}
	
	.divBlocContenu
	{
		border: 2px solid black;
		padding: 40px;
	}
		
</style>	

<div style="width:90%;margin:0 50px 0 40px;">
	<div>
	<div id="blocImage" style="text-align:center">
	{% if  app.request.getLocale()  == 'en' %}
	<img src="{{asset('uploads/logo/logobm1.jpg')}}" height="100" width="600" />
	{% else %}
		<img src="{{asset('uploads/logo/logobm.jpg')}}" height="100" width="600" />
	{% endif %}
	</div>
	<div id="blocTitre">
		<h1 class="page-header">
          {{ formulaireDelivre[0].idPole.nomFormulaire|trans }}
        </h1>
	</div>
		<br/>
    </div>
	<div id="numDossier" style="text-align:right">
	<label style="font-size:16px;">{%trans%}message_dossier_numero{%endtrans%} :</label>  <label class="valeurInput">{{ numeroDossier}}</label>
	</div>
	<br/><br/>
	<label class="labelTitreBloc">{%trans%}numero_identification_national{%endtrans%}</label>
	<br/><br/>
	<div class="divBlocContenu">
		{% for a in formulaireDelivre %}	
				<label class="titreInput">{{ a.libelle }} :</label>  <label class="valeurInput">{{ a.valeur }}</label> 	
				<br/><br/>				
		{% endfor %}
		
		<label class="titreInput">{%trans%}message_date_creation{%endtrans%} :</label>  <label class="valeurInput">{{ date|date('d-m-Y')}}</label> 
	</div>
	<br />
	<label class="labelTitreBloc">{%trans%}identification_entreprise{%endtrans%}</label>
	<br/><br/>
	<div class="divBlocContenu">
		<label class="titreInput">{%trans%}denomination{%endtrans%} :</label> <label class="valeurInput">{{ denominationSociale }}</label>
		<br/><br/>
		<label class="titreInput">{%trans%}nom_commercial{%endtrans%} :</label>  <label class="valeurInput">{{ nomCommercial }}</label>
		<br/><br/>
		<label class="titreInput">{%trans%}forme_juridique{%endtrans%} :</label>  <label class="valeurInput">{{ formeJuridique }}</label>
		<br/><br/>
		<label class="titreInput">{%trans%}capital_social{%endtrans%} :</label>  <label class="valeurInput">{{ capitalSocial }}</label>
		<br/><br/>
		<label class="titreInput">{%trans%}secteur_activite{%endtrans%} :</label>  <label class="valeurInput">{{ secteurActivite }}</label>
		<br/><br/>
		<label class="titreInput">{%trans%}adresse{%endtrans%} :</label>  <label class="valeurInput">{{ adresse }}</label>
		<br/><br/>
		<label class="titreInput">{%trans%}telephone{%endtrans%} :</label>  <label class="valeurInput">{{ telephone }}</label>
		<br/>		
	</div>	
</div>                
</body>
 
      