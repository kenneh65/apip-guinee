{% xlsdocument
    {
    'category': 'Brouillard de caisse',
    'company': 'APIP-GUINEE',
    'creator': 'APIP-GUINEE',
    'defaultStyle': {
    'font': {
        'size': 18.0
    }
},
    'description': 'Statistique de dossier',
    'keywords': 'APIP-GUINEE',
    'lastModifiedBy': 'APIP-GUINEE',
    'manager': 'Kante Mohamed',
    'security': {
    'lockRevision': true,
    'lockStructure': true,
    'lockWindows': true,
    'revisionsPassword': 'test',
    'workbookPassword': 'test'
},
    'subject': 'Liste des depots',
    'title': 'liste des dossiers deposer'
    } %}
    {% xlssheet 'Liste des dossiers'
        {
            pageMargins: {
            top: 1,
            bottom: 1,
            left: 0.75,
            right: 0.75,
            header: 0.5,
            footer: 0.5
        },
            pageSetup: {
            fitToHeight: 1,
            fitToPage: false,
            fitToWidth: 1,
            horizontalCentered: false,
            orientation: 'landscape',
            paperSize: 9,
            printArea: 'A1:B1',
            scale: 100,
            verticalCentered: false
        },

            printGridlines: true,
            rightToLeft: false,
            rowDimension: {
            'default': {
                collapsed: false,
                outlineLevel: 0,
                rowHeight: -1,
                rowIndex: '1',
                visible: true,
                xfIndex: 0,
                zeroHeight:false
            },
        },
            sheetState: 'visible',
            showGridlines: true,
            tabColor: 'a1a1e0',
            zoomScale: 75
        } %}
        {% xlsrow %}
            {% xlscell { style: { font: { size: '16' } } } %}
                {{ 'LISTE DES DOSSIERS DEPOSE DU ' }} { dateDebut|date('d/m/Y') }}  au {{ dateFin|date('d/m/Y') }}
            {% endxlscell %}
        {% endxlsrow %}
            {% xlsrow %}
                {% xlscell  { style: { font: { size: '18' } } } %}{{ 'N&deg;'|raw }}{% endxlscell %}
                {% xlscell  { style: { font: { size: '18' } } } %}{{ 'Num.dossier' }}{% endxlscell %}
                {% xlscell  { style: { font: { size: '18' } } } %}{{ 'Dénomination' }}{% endxlscell %}
                {% xlscell  { style: { font: { size: '18' } } } %}{{ 'Forme juridique' }}{% endxlscell %}
                {% xlscell  { style: { font: { size: '18' } } } %}{{ 'Date cr&eacute;ation'|raw }}{% endxlscell %}
                {% xlscell  { style: { font: { size: '18' } } } %}{{ 'Agent de dépôt'|raw }}{% endxlscell %}
            {% endxlsrow %}
            {% for d in statDepot %}
                {% xlsrow %}
                    {% xlscell %}{{ loop.index}}{% endxlscell %}
                    {% xlscell  { style: { font: { size: '18' } } } %}{{ d.numeroDossier }}{% endxlscell %}
                    {% xlscell  { style: { font: { size: '18' } } } %}{{ d.denominationSociale}}{% endxlscell %}
                    {% xlscell  { style: { font: { size: '18' } } } %}{{d.libelle }}{% endxlscell %}
                    {% xlscell  { style: { font: { size: '18' } } } %}{{ d.dateCreation|date('d/m/Y') }}{% endxlscell %}
                    {% xlscell  { style: { font: { size: '18' } } } %}{{ d.username }}{% endxlscell %}
                {% endxlsrow %}
            {% endfor %}
    {% endxlssheet %}
{% endxlsdocument %}