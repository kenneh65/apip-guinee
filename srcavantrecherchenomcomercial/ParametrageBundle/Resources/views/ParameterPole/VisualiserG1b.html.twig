<page  backtop="24mm" backbottom="6mm" backleft="10mm" backright="10mm">
<style>
        body{
            padding:14mm 10mm 4mm 10mm;
            width:672px;
            font-family: Arial;
            font-size: 9pt;
        }
        .divFieldSet
        {
            border:1px black solid;                         
            /*position:relative;*/
            padding: 5px ;
            width: 672px;
            font-size: 8pt;
        }

        .titreFielset{
            margin: -10px 30% 0 30%;
            margin-left:150px;
            font-weight: bold;
            text-align: left;
        }
        hr { 
            display: block;
            margin-top: 0.5em;
            margin-bottom: 0.5em;
            margin-left: auto;
            margin-right: auto;
            border-style: inset;
            border-width: 0.7px;
        } 
        div.case{
            border: 1px solid red;
            width: 3mm;
            height: 3mm;
        }     

        #tabAssocie th{         
            border: 1px solid;  
            height: 9px;
        }
        #tabAssocie td{
            height: 9px;
            border: 1px solid;              
        } 
        #tabDirigeant th{ 
            border: 1px solid;              
        }
        #tabDirigeant td{
            height: 9px;
            border: 1px solid;              
        } 
        #tabCommissaire th{ 
            height: 9px;
            border: 1px solid;              
        }
        #tabCommissaire td{   
            height: 8px;
            border: 1px solid;              
        } 
    </style>
     <div class="col-lg-12" style="font-size: 8pt"> 
         <div cellspacing="0" class="divFieldSet">
            <table cellspacing="0" style="width:662px; text-align: left;">
                <tr>
                    <td style="width: 132px;">
                        <table>
                            <tr>
                                <td style="font-weight: bold;font-size: 10pt">
                                    M0			
                                </td>
                            </tr>
                            <tr>
                                <td  style="font-size: 7pt">AP. Porto Novo 23/24 juin</td>
                            </tr>            
                            <tr>
                                <td  style="font-size: 7pt">1999</td>
                            </tr>
                        </table>
                    </td>
                    <td style="width: 530px;font-size: 9pt;">
                        <table>
                            {% for typeDeFormalite in listeTypeFormalite%}	
                                <tr>
                                    {% if loop.first%}
                                        <td style="font-weight: bold;font-size: 9pt"> DECLARATION : </td>
                                    {%else%}
                                        <td></td>
                                    {%endif%}
                                    {% if rccm and rccm.typeFormaliteRccm.id==typeDeFormalite.id %}
                                        <td><img src="{{asset('img/checked.png')}}"/></td>
                                        {%else%}
                                        <td><img src="{{asset('img/unchecked.png')}}"/></td>                                 
                                        {%endif%}
                                    <td>
                                        {% if rccm and rccm.typeFormaliteRccm.id==typeDeFormalite.id %}
                                            <b>{{typeDeFormalite.libelle}}</b>
                                        {%else%}
                                            {{typeDeFormalite.libelle}}
