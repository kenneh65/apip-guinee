<h1>Liste des demandes de creation d'entreprises</h1>
<div class="container table-responsive table table-striped table-hover table-bordered" style="overflow:auto">
    <table class="table table-striped table-hover">
        <thead>
        <tr>
            <th>nom</th>
            <th>prenom</th>
            <th>date naissance</th>
            <th>adresse</th>
            <th>Numero CNI</th>
            <th>tel fixe</th>
            <th>tel portable</th>
            <th>email</th>
            <th>genre</th>
            <th>situation matrimoniale</th>
			<th>conjoint</th>
			<th>pays</th>
			<th>fonction</th>
            <th>dossier demande</th>
			<th></th>
			
        </tr>
        </thead>
        {% for representant in representants %}
     <tr>	  
          <td>{{ representant.nom }}</td>
          <td>{{ representant.prenom }}</td>
          <td>{% if representant.dateDeNaissance %}{{ representant.dateDeNaissance|date('d-m-Y') }}{% endif %}</td>
          <td>{{ representant.adresse }}</td>
          <td>{{ representant.numeroIdentiteNational }}</td>
          <td>{{ representant.telephone }}</td>
         <td>{{ representant.portable }}</td>
         <td>{{ representant.email }}</td>
         <td>{{ representant.genre.code }}</td>
		 <td>{{ representant.situationMatrimoniale.code }}</td>
		<td>{{ representant.conjoint.nom }}</td>
		<td>{{ representant.pays.code }}</td>
		<td>{{ representant.fonction.code }}</td>
		<td>{{ representant.dossierDemande.id }}</td>
          <td><a href="#" title="modifier">modifier<i class="glyphicon glyphicon-pencil"></i></a>|<a href="#" title="supprimer"><i class="glyphicon glyphicon-remove"></i></a></td>
          <td></td>
     </tr>
        {% else %} <tr><td>Aucun element n'a ete trouvee.</td></tr>
        {% endfor %}
    </table>

</div>