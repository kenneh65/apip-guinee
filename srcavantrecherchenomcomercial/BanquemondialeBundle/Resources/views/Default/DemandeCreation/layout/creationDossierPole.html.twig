{% extends "::base.html.twig" %}
{% block title %}{%trans%}identification_page_title{%endtrans%}{% endblock %}

{% block body %}
    <div class="hpanel">
        <div class="panel-heading">
            <h1 class="page-header">
                {%trans%}identification_page_title{%endtrans%}<label class="pull-right h4" style="color: #d44" >{%trans%}message_dossier_numero{%endtrans%} {{creationdossier.numeroDossier}}</label>
            </h1>
            {% block sousMenu %}
                {{ render(controller("BanquemondialeBundle:Shared:etapeCreationPole",{'idd':idd})) }}
            {% endblock %}
        </div>
        <div class="panel-body table-bordered">

            <div class="row">
                <div class="form-group col-sm-4">
                    <label>{%trans%}type_operation{%endtrans %}</label>
                    <br/>
                    {%if definedTypeOperation %} {{ definedTypeOperation.libelle }} {% endif %}
                </div>
                <div class="form-group col-sm-4">
                    <label>{%trans%}forme_juridique{%endtrans %}</label>
                    <br/>
                    {%if definedFormeJuridiqueTraduction %} {{ definedFormeJuridiqueTraduction.libelle }} {% endif %}
                </div>
                <div class="form-group col-sm-4">
                    <label>{%trans%}type_dossier{%endtrans %}</label>
                    <br/>
                    {{ creationdossier.typeDossier.libelle }}
                </div>
            </div>
            <div class="row">        
                <div class="form-group col-sm-4">
                    <label>{%trans%}denomination{%endtrans %}</label>
                    <br/>
                    {{ creationdossier.denominationSociale }}
                </div>
                <div class="form-group col-sm-4">
                    <label>{%trans%}nom_commercial{%endtrans %}</label>
                    <br/>
                    {{ creationdossier.nomCommercial }}
                </div>
                <div class="form-group col-sm-4">
                    <label>{%trans%}pays{%endtrans %}</label>
                    <br/>
                    {%if definedPaysTraduction %} {{ definedPaysTraduction.libelle }} {% endif %}
                </div>

            </div>
            <div class="row">                
                <div class="form-group col-sm-4">
                    <label>{%trans%}region{%endtrans %}</label>
                    <br/>
                    {{ creationdossier.region }}
                </div>
                <div class="form-group col-sm-4">
                    <label>{%trans%}prefecture{%endtrans %}</label>
                    <br/>
                    {{ creationdossier.prefecture.libelle }}
                </div>
                <div class="form-group col-sm-4">
                    <label>{%trans%}sousPrefecture{%endtrans %}</label>
                    <br/>
                    {{ creationdossier.sousPrefecture.libelle }}
                </div>
            </div>
            <div class="row">
                <div class="form-group col-sm-4">
                    <label>{%trans%}ville{%endtrans %}</label>
                    <br/>
                    {%if creationdossier.ville%}{{ creationdossier.ville|upper }}{%endif%}
                </div> 
                <div class="form-group col-sm-4">
                    <label>{%trans%}en_activite{%endtrans %}</label>
                    <br/>
                    {{ creationdossier.enActivite }}
                </div>
                <div class="form-group col-sm-4">
                    <label>{%trans%}date_debut{%endtrans %}</label>
                    <br/>
                    {{ creationdossier.dateDebut|date('d-m-Y') }}
                </div>                                
            </div>
            <div class="row">                        
                <div class="form-group col-sm-4">
                    <label>{%trans%}activite_principale{%endtrans %}</label>
                    <br/>
                    {%if secteurActivite %} {{ secteurActivite.libelle }} {% endif %}
                </div>
                <div class="form-group col-sm-4">
                    <label>{%trans%}activite_secondaire{%endtrans %}</label>
                    <br/>
                    {%if creationdossier.activiteSecondaire %} {{ creationdossier.activiteSecondaire }} {% endif %}
                </div>
                <div class="form-group col-sm-4">
                    <label>{%trans%}activite_secondaire{%endtrans %} 2</label>
                    <br/>
                    {%if creationdossier.activiteSecondaire2 %} {{ creationdossier.activiteSecondaire2 }} {% endif %}
                </div>
            </div>                

            <div class="row">
                <div class="form-group col-sm-4">
                    <label>{%trans%}sigle{%endtrans %}</label>
                    <br/>
                    {{ creationdossier.sigle }}
                </div>
                <div class="form-group col-sm-4">
                    <label>{%trans%}enseigne{%endtrans %}</label>
                    <br/>
                    {{ creationdossier.enseigne }}
                </div>
                <div class="form-group col-sm-4">
                    <label>{%trans%}duree{%endtrans %}</label>
                    <br/>
                    {{ creationdossier.duree }}
                </div>                
            </div>
            <div class="row">

                <div class="form-group col-sm-4">
                    <label>{%trans%}phone{%endtrans %}</label>
                    <br/>
                    {{ creationdossier.telephone }}
                </div>
                <div class="form-group col-sm-4">
                    <label>{%trans%}phone{%endtrans %} 2</label>
                    <br/>
                    {{ creationdossier.telephone2 }}
                </div>
                <div class="form-group col-sm-4">
                    <label>{%trans%}email{%endtrans %}</label>
                    <br/>
                    {{ creationdossier.email }}
                </div>

            </div> 
            <div class="row">
                <div class="form-group col-sm-4">
                    <label>{%trans%}boite_postale{%endtrans %}</label>
                    <br/>
                    {{ creationdossier.boitePostale }}
                </div>
                <div class="form-group col-sm-8">
                    <label>{%trans%}adresse{%endtrans %}</label>
                    <br/>
                    {{ creationdossier.adresseSiege }}
                </div>

            </div>
            <div class="row">        
                <div class="form-group col-sm-6">
                    <label>{%trans%}activite_sociale{%endtrans %}</label>
                    <br/>
                    {{ creationdossier.activiteSociale }}<br>
                    {{creationdossier.activiteComplementaire}}
                </div>
                
            </div>


            <div class="row pull-right">

                {% if routeAvant %}<input class="btn btn-primary btn-goto" style="width: 100px" value="{{'precedent'|trans}}" type="button" data-url="{{path(routeAvant,{'idd':idd})}}" />{% endif %}
                {% if rteSuivant %}<input class="btn btn-info btn-goto" style="width: 100px" value="{{'suivant'|trans}}" type="button" data-url="{{path(rteSuivant,{'idd':idd})}}" />{% endif %}
            </div>

        </div>


    </div>


{% endblock %}  
{%block javascripts %}
    {{parent()}}
    <script type="text/javascript">
        $(".btn-goto").click(function () {
            var url = $(this).attr('data-url');
            window.location.href = url;
        });
    </script>
{%endblock %}